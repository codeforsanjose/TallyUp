{
  "name": "tally-up-backend",
  "description": "tally-up-backend",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.840.0",
    "@aws-sdk/client-ses": "^3.839.0",
    "@aws-sdk/dsql-signer": "^3.835.0",
    "@neondatabase/serverless": "^1.0.1",
    "@node-rs/argon2": "^2.0.2",
    "drizzle-orm": "^0.44.2",
    "jsonwebtoken": "^9.0.2",
    "yaml": "^2.8.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@aws-sdk/client-cloudformation": "^3.835.0",
    "@aws-sdk/client-lambda": "^3.835.0",
    "@aws-sdk/client-s3": "^3.842.0",
    "@aws-sdk/credential-provider-ini": "^3.835.0",
    "@aws-sdk/credential-providers": "^3.835.0",
    "@aws-sdk/types": "^3.821.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/bun": "^1.2.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/yargs": "^17.0.33",
    "archiver": "^7.0.1",
    "drizzle-kit": "^0.31.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ws": "^8.18.3",
    "yargs": "^17"
  },
  "scripts": {
    "build": "bun run scripts/build.ts",
    "dev": "bun --hot --define=process.env.NODE_ENV:\"'development'\" run scripts/dev/index.ts ",
    "deploy": "bun run scripts/deploy",
    "gen-spec": "bun run scripts/gen-spec.ts",
    "format": "bun run prettier --write .",
    "postinstall": "bun run scripts/postinstall",
    "push-schema": "DATABASE_URL=postgres://postgres@localhost:5432/postgres bunx drizzle-kit push"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.mjs?(x)",
      "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js"
    ]
  }
}