{
  "name": "tally-up-backend",
  "description": "tally-up-backend",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.799.0",
    "@aws-sdk/client-dynamodb": "^3.398.0",
    "@aws-sdk/lib-dynamodb": "^3.398.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/bun": "^1.2.10",
    "prettier": "^3.5.3",
    "yaml": "^2.7.1"
  },
  "scripts": {
    "build": "bun run scripts/build.ts",
    "gen-client": "bun run scripts/gen-spec.ts",
    "format": "bun run prettier --write ."
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.mjs?(x)",
      "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js"
    ]
  },
  "workspaces": {
    "packages": [
      "htmx-pwa-frontend"
    ]
  }
}